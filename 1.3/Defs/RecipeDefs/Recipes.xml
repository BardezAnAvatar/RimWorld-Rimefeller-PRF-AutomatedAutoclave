<Defs>

  <RecipeDef>
    <defName>CureComposite</defName>
    <label>Cure Synthamide Composite</label>
    <description>Use heat and pressure to cure uncured composite.</description>
    <jobString>Curing</jobString>
    <effectWorking>Cook</effectWorking>
    <workSkill>Cooking</workSkill>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <requiredGiverWorkType>Cooking</requiredGiverWorkType>
    <workAmount>30000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>UncuredComposite</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>UncuredComposite</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <FiberComposite>50</FiberComposite>
    </products>
  </RecipeDef>

